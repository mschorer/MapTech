				<mx:Button label="zoom +" width="50%" buttonDown="tl.zoomIn();" repeatInterval="100" repeatDelay="800" autoRepeat="true"/>
			</mx:VBox>
			<mx:ProgressBar width="100%" id="pbLoad"/>
